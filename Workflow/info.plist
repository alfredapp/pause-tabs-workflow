				<string>const chromiumVariants = ["Google Chrome", "Chromium", "Opera", "Vivaldi", "Brave Browser", "Microsoft Edge"]
const webkitVariants = ["Safari", "Webkit", "Orion"]
const browserVariants = chromiumVariants.concat(webkitVariants)

const runningAppNames = Application("System Events")
  .applicationProcesses.where({ backgroundOnly: false })()
  .map(app =&gt; app.name())

if (runningAppNames.find(appName =&gt; appName.startsWith("firefox")))
  console.log("Firefox is not scriptable")

runningAppNames
  .filter(appName =&gt; browserVariants.some(variant =&gt; appName.startsWith(variant)))
  .join(",")</string>
