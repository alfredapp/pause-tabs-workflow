				<key>script</key>
				<string>// Grab environment variables
function envVar(varName) {
  return $.NSProcessInfo
    .processInfo
    .environment
    .objectForKey(varName).js
}

const actOn = envVar("act_on")
const skipActive = envVar("skip_active") === "1"
const frontmostName = envVar("frontmost_name")
const runningBrowsers = envVar("running_browsers").split(",")

// Code to pause
const pausePlayer = `
  document.querySelectorAll("video, audio").forEach(e =&gt; e.pause())

  // Pause embedded YouTube players
  document.querySelectorAll("iframe").forEach(e =&gt; {
    if (e.getAttribute("src").indexOf("youtube.com/embed") === -1) return

    e.contentWindow.postMessage(JSON.stringify({
      "event": "command",
      "func": "pauseVideo"
    }), "*")
  })
`

// Set up
const webkitVariants = ["Safari", "Webkit", "Orion"]
const chromiumVariants = ["Google Chrome", "Chromium", "Opera", "Vivaldi", "Brave Browser", "Microsoft Edge"]

const targetBrowsers = (actOn === "all") ? runningBrowsers : [frontmostName]

// Pause tabs
targetBrowsers.forEach(browserName =&gt; {
  browser = Application(browserName)

  // Webkit variants
  if (webkitVariants.some(variant =&gt; browserName.startsWith(variant))) {
    browser.windows()?.forEach(browserWindow =&gt; {
      browserWindow.tabs()?.forEach(browserTab =&gt; {
        // Skip current tab if conditions are met
        if (skipActive &amp;&amp; frontmostName === browserName &amp;&amp; browserWindow.id() === browser.windows()[0].id() &amp;&amp; browserTab.index() === browserWindow.currentTab.index()) return
        // Pause tab
        browser.doJavaScript(pausePlayer, {in: browserTab})
      })
    })
  // Chromium Variants
  } else if (chromiumVariants.some(variant =&gt; browserName.startsWith(variant))) {
    browser.windows()?.forEach(browserWindow =&gt; {
      browserWindow.tabs()?.forEach(browserTab =&gt; {
        // Skip current tab if conditions are met
        if (skipActive &amp;&amp; frontmostName === browserName &amp;&amp; browserWindow.id() === browser.windows()[0].id() &amp;&amp; browserTab.id() === browserWindow.activeTab.id()) return
        // Pause tab
        browserTab.execute({javascript: pausePlayer})
      })
    })
  } else {
    throw new Error(browserName + " is not a supported browser")
  }
})</string>
